{
  "name": "mojang-api",
  "version": "1.0.0",
  "description": "Fast caching layer for the Mojang API.",
  "author": "Ashcon Partovi",
  "email": "ashcon@partovi.net",
  "keywords": [
    "minecraft",
    "mojang",
    "api"
  ],
  "homepage": "https://github.com/Electroid/mojang-api",
  "repository": {
    "type": "git",
    "url": "github:Electroid/mojang-api"
  },
  "license": "MIT",
  "main": "src/index.coffee",
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack --watch",
    "preview": "workers-preview < dist/bundle.js",
    "test": "jest"
  },
  "dependencies": {},
  "devDependencies": {
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.3.1",
    "jest": "^23.4.1",
    "jest-coffee-preprocessor": "^1.0.0",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8",
    "webpack-command": "^0.4.0",
    "workers-preview": "^1.0.4"
  },
  "jest": {
    "verbose": true,
    "testRegex": "/test/.*\\.coffee",
    "transform": {
      "^.+\\.coffee$": "<rootDir>/node_modules/jest-coffee-preprocessor/index.js"
    },
    "moduleFileExtensions": [
      "coffee",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
